@model System.Collections.Generic.IEnumerable<Client2.Models.Booking>

@{
    Layout = null;
}

<!DOCTYPE html>
<h2>List of Booking</h2>
<div class="container">
    <table class="table table-hover">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Client Full Name</th>
            <th scope="col">RoomType id/th>
            <th scope="col">CheckIn</th>
            <th scope="col">CheckOut</th>
            <th scope="col">CanceledAt</th>
            <th scope="col">IsCanceled</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var forecast in this.Model)
        {
            <tr>
                <th scope="row">@forecast.Id</th>
                <td>@forecast.Client.FullName</td>
                <td>@forecast.Room.Type</td>
                <td>@forecast.CheckIn</td>
                <td>@forecast.CheckOut</td>
                <td>@forecast.CanceledAt</td>
                <td>@forecast.IsCanceled</td>
            </tr>
        }
        </tbody>
    </table>
</div>