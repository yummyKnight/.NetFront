@model System.Collections.Generic.IEnumerable<Client2.Models.Payment>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
<h2>List of Booking</h2>
<div class="container">
    <table class="table table-hover">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Amount/th>
            <th scope="col">Booking ID/th>
            <th scope="col">IsCanceled</th>
            <th scope="col">CanceledAt</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var forecast in this.Model)
        {
            <tr>
                <th scope="row">@forecast.Id</th>
                <td>@forecast.Amount</td>
                <td>@forecast.Booking.Id</td>
                <td>@forecast.IsCanceled</td>
                <td>@forecast.CanceledAt</td>
            </tr>
        }
        </tbody>
    </table>
</div>
</body>
</html>